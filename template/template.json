{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "managedClusters_k8s_name": {
            "defaultValue": "k8s",
            "type": "String"
        },
        "managedClusters_k8s_loganalyticsworkspaceresourceid": {
            "defaultValue": "/subscriptions/62576bd7-def3-4533-9d2b-b9479ac76b5b/resourcegroups/defaultresourcegroup-eus/providers/microsoft.operationalinsights/workspaces/defaultworkspace-62576bd7-def3-4533-9d2b-b9479ac76b5b-eus",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.ContainerService/managedClusters",
            "name": "[parameters('managedClusters_k8s_name')]",
            "apiVersion": "2018-03-31",
            "location": "westus2",
            "scale": null,
            "properties": {
                "kubernetesVersion": "1.12.6",
                "dnsPrefix": "myapp",
                "agentPoolProfiles": [
                    {
                        "name": "agentpool",
                        "count": 1,
                        "vmSize": "Standard_B2s",
                        "osDiskSizeGB": 30,
                        "storageProfile": "ManagedDisks",
                        "maxPods": 110,
                        "osType": "Linux"
                    }
                ],
                "servicePrincipalProfile": {
                    "clientId": "ed7e3665-d920-4efb-a3d6-6bef91841888"
                },
                "addonProfiles": {
                    "httpapplicationrouting": {
                        "enabled": false,
                        "config": {}
                    },
                    "omsagent": {
                        "enabled": true,
                        "config": {
                            "loganalyticsworkspaceresourceid": "[parameters('managedClusters_k8s_loganalyticsworkspaceresourceid')]"
                        }
                    }
                },
                "enableRBAC": true,
                "networkProfile": {
                    "networkPlugin": "kubenet",
                    "podCidr": "10.244.0.0/16",
                    "serviceCidr": "10.0.0.0/16",
                    "dnsServiceIP": "10.0.0.10",
                    "dockerBridgeCidr": "172.17.0.1/16"
                }
            },
            "dependsOn": []
        }
    ]
}
